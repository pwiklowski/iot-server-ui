server {
	listen 7000 default_server;
	listen [::]:7000 default_server;


	root /webui;

	# Add index.php to the list if you are using PHP
	index index.html 

	server_name _;


	location /iot/ {
		proxy_pass http://iotserver:7000/;
	}
	location /api/ {
		proxy_pass http://dbbackend:7001/;
	}
	location /ws/ {
		proxy_pass http://iotserver:7002/;
        	proxy_http_version 1.1;
        	proxy_set_header Upgrade $http_upgrade;
        	proxy_set_header Connection "upgrade";
	}
}
